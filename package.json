    "android",
    "ios",
    "keychain",
    "shared-preferences"
  ],
  "author": "Randy Coulman <randy@codingzeal.com> (http://open.codingzeal.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodingZeal/redux-persist-sensitive-storage/issues"
  },
  "homepage": "https://github.com/meshleteam/redux-persist-sensitive-storage#readme",
  "peerDependencies": {
    "react-native": "*",
    "react-native-sensitive-info": "3.x - 6.x"
  },
  "devDependencies": {
    "@types/react-native": "^0.63.48",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-zeal": "^2.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^3.6.1",
    "npm-run-all": "^4.0.2",
    "prettier": "^2.2.1",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.45.1",
    "react-native-sensitive-info": "^6.0.0-alpha.9",
    "testdouble": "^3.0.0",
    "typescript": "^4.1.4"
  },
  "jest": {
    "preset": "react-native",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js"
  },
  "lint-staged": {
    "*.ts": "prettier --list-different"
  }
