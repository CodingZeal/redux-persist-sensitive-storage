{
  "name": "@meshleteam/redux-persist-sensitive-storage",
  "version": "1.0.2",
  "description": "redux-persist storage engine for react-native-sensitive-info",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint '**/*.ts' --max-warnings 0",
